import java.util.*;
class employee
{
 String name;
 int id,yoe,designation;
 double basic_pay,da,hra,lic,pf,working_hrs,hourly_wages,deductions,gross_salary,net_salary;
    
 void grosspay()
  {
  if(designation==1)
    { da=2000;
      hra=1000;
      gross_salary=working_hrs*hourly_wages+da+hra;
       if(lic==0)
         deductions=500;
       else
          deductions=lic+500;
    
          
    }
   else if(designation==2)
    {
      da=basic_pay*0.4;
      hra=basic_pay*0.1;
      pf=basic_pay*0.8;
    gross_salary=basic_pay+da+hra;
    if(lic==0)
         deductions=pf;
       else
          deductions=lic+pf;
  
    
   }
   
  else if(designation==3)
    {
      da=basic_pay*0.3;
      hra=basic_pay*0.1;
      pf=basic_pay*0.8;
      gross_salary=basic_pay+da+hra;
    if(lic==0)
         deductions=pf;
       else
          deductions=lic+pf;
  
   
    }
 }
 void netpay()
   {
    net_salary=gross_salary-deductions;
   }
  
}

class emp
